A {  font-family: "宋体"; font-size: 9pt; color : black; 
  text-decoration : none;
}
A:HOVER {  font-family: "宋体"; font-size: 9pt; color : #ff8000; text-decoration: none;}
A:LINK {  font-family: "宋体"; font-size: 9pt; color : black; 
  text-decoration : none;
}

.white {  font-family: "宋体"; font-size: 9pt; color: #FFFFFF;}
input {  font-family: "宋体"; font-size: 9pt; color: #000000; border: 1px #000000 solid; background-color: #FFFFFF;}
.repeatx {  background-repeat: repeat-x;}
select {  font-family: "宋体"; font-size: 9pt; color: #000000; border: #000000; border-style: solid; border-top-width: 1px; border-right-width: 1px; border-bottom-width: 1px; border-left-width: 1px;}
.leftin {  margin-left: 8px;}
.hand {  cursor: hand}
.leftin20 {  margin-left: 20px;}
.repeaty {  background-repeat: repeat-y;}
IFRAME{border-width : 0px 0px 0px 0px;}
A.w{
  font-size : 12px;
  font-family : 宋体;
  color : white;
  text-decoration : underline;
}
A.w:hover{
  font-size : 12px;
  font-family : 宋体;
  text-decoration : none;
  color : #fdac40;
}
A.w:link{
  font-size : 12px;
  font-family : 宋体;
  color : white;
  text-decoration : underline;
}
A:ACTIVE{
  color : #ff8000;
  text-decoration : none;
}
body {  font-family: "宋体"; font-size: 9pt; color: #000000; background-color: #ffffff; margin-top: 0px; margin-left: 0px;margin-right: 0px; margin-bottom: 0px;
}
td {  font-family: "宋体"; font-size: 9pt; color: #000000;font-weight:normal;}
.lable_down{  font-family: "宋体"; font-size: 9pt; color: #57649B;  text-align:center;background-color: #DEE9F4; border-color: #FFFFFF #DEE9F4 #DEE9F4 #ffffff;line-height: 18px; ; border-style: solid; border-top-width: 1px; border-right-width: 1px; border-bottom-width: 1px; border-left-width: 1px;cursor: hand}
.lable_up {  font-family: "宋体"; font-size: 9pt; color: #766339;  text-align:center;background-color: #F0EAE2; border-color: #ffffff #F0EAE2 #F0EAE2 #ffffff;line-height: 18px; ; border-style: solid; border-top-width: 1px; border-right-width: 1px; border-bottom-width: 1px; border-left-width: 1px;cursor:hand}
.lable_normal {  font-family: "宋体"; font-size: 9pt; color: #57649B;  text-align: center;background-color: #DEE9F4; border-color: #FFFFFF #DEE9F4 #DEE9F4 #ffffff;line-height: 18px; ; border-style: solid; border-top-width: 1px; border-right-width: 1px; border-bottom-width: 1px; border-left-width: 1px;}


